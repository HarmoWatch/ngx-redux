{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@harmowatch/ngx-redux-core",
  "version": "0.1.28-2",
  "license": "MIT",
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "replace": "replace",
    "build:package": "ng-packagr -p package.json",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "publish:package": "npm run build:package && npm publish ./dist/ --access=public --registry https://registry.npmjs.org/",
    "ci:test": "ng test --code-coverage --single-run && npm run coveralls",
    "ci:lint": "ng lint",
    "ci:build:github:page": "ng build --output-path github-page --deploy-url=https://harmowatch.github.io/ngx-redux-core/ --base-href=/ngx-redux-core/"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/ngx-redux/public_api.ts",
      "languageLevel": [
        "dom",
        "es2017"
      ]
    }
  },
  "private": false,
  "keywords": [
    "angular",
    "angular 2",
    "angular 4",
    "angular2",
    "angular4",
    "redux",
    "ng-redux",
    "ng",
    "annotation",
    "decorator"
  ],
  "author": "Kay Schecker",
  "bugs": {
    "url": "https://github.com/HarmoWatch/ngx-redux-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HarmoWatch/ngx-redux-core.git"
  },
  "homepage": "https://github.com/HarmoWatch/ngx-redux-core#readme",
  "peerDependencies": {
    "@angular/core": ">=5.0.0",
    "redux": ">=3.0.0"
  },
  "devDependencies": {
    "@angular/cli": "1.5.3",
    "@angular/common": "5.2.0",
    "@angular/compiler": "5.2.0",
    "@angular/compiler-cli": "5.2.0",
    "@angular/core": "5.2.0",
    "@angular/forms": "5.2.0",
    "@angular/http": "5.2.0",
    "@angular/language-service": "5.2.0",
    "@angular/platform-browser": "5.2.0",
    "@angular/platform-browser-dynamic": "5.2.0",
    "@angular/router": "5.2.0",
    "@types/jasmine": "2.8.4",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.5.9",
    "@types/redux-logger": "3.0.5",
    "codelyzer": "4.1.0",
    "core-js": "2.5.3",
    "coveralls": "3.0.0",
    "jasmine-core": "2.9.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.3",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "ng-packagr": "2.0.0-rc.11",
    "protractor": "5.2.2",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "rxjs": "5.5.6",
    "ts-node": "3.3.0",
    "tslint": "5.9.1",
    "typescript": "2.4.2",
    "zone.js": "0.8.20"
  },
  "dependencies": {
    "@harmowatch/redux-decorators": "0.0.3"
  }
}
